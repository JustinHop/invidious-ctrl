{
  "manifest_version": 2,
  "name": "HookTube Ctrl",
  "description": "Enables youtube style keyboard controls on HookTube",
  "version": "0.1",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://hooktube.com/watch?*",
        "*://*.hooktube.com/watch?*"
      ],
      "run_at": "document_end",
      "js": ["main.js"]
    }
  ]
}
